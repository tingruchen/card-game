{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"],
      "assets": ["src/assets/*"],
      "components": ["src/components/*"],
      "utility": ["src/utility/*"],
      "tailwind": ["src/tailwind/*"],
      "pages": ["src/pages/*"],
      "apis": ["src/api/*"],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
